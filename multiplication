import java.util.*;
public class Solution {
    public static void main(String[] args)  {
       Scanner sc=new Scanner(System.in);
       int n,i;
       n=sc.nextInt();
       for(i=1;i<=10;i++){
           int c;
           c=n*i;
           System.out.println(n+" x "+i+" = " +c);
       }
    }
}
