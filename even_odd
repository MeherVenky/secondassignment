import java.util.*;
class even_odd
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int n;
System.out.println("Enter number");
n=sc.nextInt();
int r;
int m=n;
System.out.print("even numbers-:");
while(n!=0)
{
r = n%10;

if(r%2==0){
System.out.print(r);
}

n=n/10;

}
n=m;
System.out.print();
System.out.print("odd numbers-:");
while(n!=0)
{
r = n%10;

if(r%2==1){
System.out.print(r);
}

n=n/10;

}
}
}
